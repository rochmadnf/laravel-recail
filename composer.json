{
    "name": "rochmadnf/laravel-recail",
    "description": "React Email for Laravel",
    "keywords": ["laravel", "laravel mail", "react", "react email", "email", "resend", "laravel resend", "laravel react email"],
    "homepage": "https://github.com/rochmadnf/laravel-recail",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rochmadnf\\Recail\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rochmad Nurul Fahmi",
            "email": "rochmadnf@protonmail.com",
            "role": "contributor"
        },
        {
            "name": "Jamie Schouten",
            "email": "j4mie@hey.com",
            "role": "main contributor"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rochmadnf\\Recail\\ReactEmailServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.20 || ^8.0 || ^9.0",
        "pestphp/pest": "^1.20 || ^2.0"
