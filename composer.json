{
  "name": "rochmadnf/laravel-recail",
  "description": "React Email for Laravel",
  "keywords": [
    "laravel",
    "laravel mail",
    "laravel mailable",
    "react",
    "react email",
    "email",
    "resend",
    "laravel resend",
    "laravel react email"
  ],
  "homepage": "https://github.com/rochmadnf/laravel-recail",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rochmadnf\\Recail\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Rochmad Nurul Fahmi",
      "email": "rochmadnf@protonmail.com",
      "role": "contributor"
    },
    {
      "name": "Jamie Schouten",
      "email": "j4mie@hey.com",
      "role": "main contributor"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Rochmadnf\\Recail\\ReactEmailServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.1",
    "pestphp/pest": "^2.34",
    "laravel/pint": "^1.16"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
